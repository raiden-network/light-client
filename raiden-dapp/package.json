    "pouchdb-adapter-cordova-sqlite": "^2.0.8",
    "query-string": "^7.0.1",
    "raiden-ts": "^2.0.0-rc.2",
    "rxjs": "^7.4.0",
    "tiny-async-pool": "^1.2.0",
